model Province {
  id            Int             @id @default(autoincrement())
  name          String?
  City          City[]
  DataEmotion   DataEmotion[]
  DataByContent DataByContent[]
}

model City {
  id            Int             @id @default(autoincrement())
  name          String?
  Province      Province?       @relation(fields: [provinceId], references: [id])
  provinceId    Int?
  DataEmotion   DataEmotion[]
  DataByContent DataByContent[]
  CityValue     CityValue[]
}

model Candidate {
  id            Int             @id @default(autoincrement())
  name          String?
  img           String?
  DataEmotion   DataEmotion[]
  DataByContent DataByContent[]
}

model Emotion {
  id          Int           @id @default(autoincrement())
  name        String?
  DataEmotion DataEmotion[]
}

// depracated
model DataEmotion {
  id          Int        @id @default(autoincrement())
  Candidate   Candidate? @relation(fields: [candidateId], references: [id])
  candidateId Int?
  Province    Province?  @relation(fields: [provinceId], references: [id])
  provinceId  Int?
  City        City?      @relation(fields: [cityId], references: [id])
  cityId      Int?
  Emotion     Emotion?   @relation(fields: [emotionId], references: [id])
  emotionId   Int?
  value       Int?
}

model DataByContent {
  id           Int        @id @default(autoincrement())
  Candidate    Candidate? @relation(fields: [candidateId], references: [id])
  Province     Province?  @relation(fields: [provinceId], references: [id])
  City         City?      @relation(fields: [cityId], references: [id])
  candidateId  Int?
  provinceId   Int?
  cityId       Int?
  trust        Int?
  joy          Int?
  surprise     Int?
  anticipation Int?
  sadness      Int?
  fear         Int?
  anger        Int?
  disgust      Int?
  date         DateTime?  @db.Date
  createdAt    DateTime   @default(now())
  updatedAt    DateTime   @updatedAt
}

// =====================================
// data dari google sheet
model NationWideChart {
  id       Int       @id @default(autoincrement())
  date     DateTime? @db.Date
  positive Int?
  negative Int?
  neutral  Int?
}

model SourceOfMention {
  id        Int     @id @default(autoincrement())
  media     String?
  value     Int?
  indicator Int?
}

model WordCloud {
  id    Int     @id @default(autoincrement())
  word  String?
  value Int?
}

// ====================================
// database
model CityValue {
  id     Int   @id @default(autoincrement())
  City   City? @relation(fields: [cityId], references: [id])
  cityId Int?
  value  Int?
}

